      "label": "Parties",
      "type": "text",
      "normalizer": "text",
      "hints": ["between", "parties", "agreement"],
      "search": {
        "join_groups": true,
        "patterns": [
          "(?is)by and between\\s+([^\\n\\.;]{3,200}?)\\s+and\\s+([^\\n\\.;]{3,200})",
          "(?is)this\\s+agreement\\s+is\\s+made\\s+between\\s+([^\\n\\.;]{3,200}?)\\s+and\\s+([^\\n\\.;]{3,200})"
        ]
      }
    },
    {
      "key": "effective_date",
      "label": "Effective Date",
      "type": "date",
      "normalizer": "date",
      "hints": ["effective date", "effective as of"],
      "search": {
        "patterns": [
          "(?is)effective(?:\\s+as\\s+of)?\\s+(?:date\\s*)?[:\\-]?\\s*([A-Za-z]+\\s+\\d{1,2},\\s+\\d{4})",
          "(?is)effective\\s+date\\s*[:\\-]?\\s*([0-9]{1,2}/[0-9]{1,2}/[0-9]{2,4})"
        ]
      }
    },
    {
      "key": "term",
      "label": "Term",
      "type": "text",
      "normalizer": "text",
      "hints": ["term", "termination", "expires"],
      "search": {
        "patterns": [
          "(?is)(?:term and termination|term)\\s*[\\.:\\-]\\s*([^\\n]{20,260})",
          "(?is)during the term[,\\s]+([^\\n]{20,260})"
        ]
      }
    },
    {
